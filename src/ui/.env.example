# Example environment configuration
# Copy this file to .env.local and customize for your local setup

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8003
REACT_APP_API_TIMEOUT=30000

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=50000000
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.txt

# Default Search Settings
REACT_APP_DEFAULT_TOP_K=10
REACT_APP_DEFAULT_CONFIDENCE_THRESHOLD=0.7

# System Settings
REACT_APP_POLLING_INTERVAL=30000
REACT_APP_DEBUG_MODE=false